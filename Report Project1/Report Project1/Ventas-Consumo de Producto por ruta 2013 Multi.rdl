      <rd:DataSourceID>ba550a35-99ad-4eb6-a781-6ea0459bd743</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Matrix">
      <Query>
        <DataSourceName>VentasConRuta</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@producto">
            <Value>=Parameters!producto.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ifecha">
            <Value>=Parameters!ifecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@afecha">
            <Value>=Parameters!afecha.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     r.IdZona, r.ZonaResp, r.NombreSubZona, r.NombreSupervisor, r.NombreRuta, r.IdVendedor, r.NombreVendedor, v.CUSTNMBR AS 'CodCliente', 
                      v.CUSTNAME AS 'NombCliente', v.ITEMNMBR AS 'CodProd', v.ITEMDESC AS 'itemdesc', SUM(v.QUANTITY) AS Cantidad, SUM(v.XTNDPRCE) AS total, 
                      YEAR(v.DOCDATE) AS Ano, MONTH(v.DOCDATE) AS Mes, DAY(v.DOCDATE) AS DIA
FROM         view_VentaGPruta AS v INNER  JOIN
                      PalmComSync.dbo.vw_RutaZona AS r ON v.Ruta = r.NombreRuta
WHERE     (v.ITEMNMBR IN(@producto)) AND (v.DOCDATE BETWEEN @ifecha AND @afecha) AND v.LOCNCODE like '%SAN%'
GROUP BY r.IdZona, r.ZonaResp, r.NombreSubZona, r.NombreSupervisor, r.NombreRuta, r.IdVendedor, r.NombreVendedor, v.CUSTNMBR, v.CUSTNAME, v.ITEMNMBR, 
                      v.ITEMDESC, YEAR(v.DOCDATE), MONTH(v.DOCDATE), DAY(v.DOCDATE)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IdZona">
          <DataField>IdZona</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
