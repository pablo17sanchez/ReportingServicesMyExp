    <DataSet Name="Producto">
      <Query>
        <DataSourceName>VentasConRuta</DataSourceName>
        <CommandText>SELECT     CODPROD, (CODPROD+'-'+DESCRIP) AS DESCRIP
FROM         PalmComSync.dbo.OUT_PRODUCTO
GROUP BY CODPROD, DESCRIP</CommandText>
