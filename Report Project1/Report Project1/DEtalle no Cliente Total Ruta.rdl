  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DSvisitadoNoF</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FECHA">
            <Value>=Parameters!FECHA.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@nombreRuta">
            <Value>=Parameters!nombreRuta.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT    [Codigo]
      ,[CodCliente]
      ,[Nombre]
      ,[PERS_CONTACT]
      ,[PHONE1]
      ,[PHONE2]
      ,[CodRuta]
      ,[NombreRuta]
      ,[ZonaResp]
      ,[NOMBREVEND]
      ,[Lunes]
      ,[Martes]
      ,[Miercoles]
      ,[Jueves]
      ,[Viernes]
      ,[Sabado]
      ,[Domingo]
      ,[DATE]
      ,[usuario]
      ,[FECHAMODIF]
      ,[TIPOVENDEDOR]
      ,[ID]
      ,[PALMID]
      ,[IDVENDEDOR]
      ,[FECHA], dbo.fn_devuelveFacturadoPalm(@FECHA, CodCliente, IDVENDEDOR) AS 'FACTURA', dbo.fn_devuelveVisitaPalm(@FECHA, CodCliente, 
                      PALMID) AS 'NO_VISITA'
FROM         DataRS.dbo.vw_ruta_sustitutos_vendedor_h
WHERE     (DATE = @FECHA)  AND (NombreRuta = @nombreRuta) AND dbo.fn_devuelveFacturadoPalm(@FECHA, CodCliente, IDVENDEDOR) is null AND dbo.fn_devuelveVisitaPalm(@FECHA, CodCliente, PALMID) is null
GROUP BY [Codigo]
      ,[CodCliente]
      ,[Nombre]
      ,[PERS_CONTACT]
      ,[PHONE1]
      ,[PHONE2]
      ,[CodRuta]
      ,[NombreRuta]
      ,[ZonaResp]
      ,[NOMBREVEND]
      ,[Lunes]
      ,[Martes]
      ,[Miercoles]
      ,[Jueves]
      ,[Viernes]
      ,[Sabado]
      ,[Domingo]
      ,[DATE]
      ,[usuario]
      ,[FECHAMODIF]
      ,[TIPOVENDEDOR]
      ,[ID]
      ,[PALMID]
      ,[IDVENDEDOR]
      ,[FECHA]
ORDER BY CodRuta</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
