        <Field Name="CANTIDAD">
          <DataField>CANTIDAD</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PRECIO">
          <DataField>PRECIO</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>GPDB</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT 
	s.IdZona,
	c.CodCliente,
	v.CUSTNAME,
	c.NombreRuta,
	MONTH(v.docdate)Mes,
	YEAR(v.docdate)ANO,
	SUM(v.QUANTITY) CANTIDAD,
	SUM(v.XTNDPRCE) PRECIO 
FROM PalmComSync..GP_CLIENTEDIASRUTABAK c 
JOIN View_VentasGP2 v ON c.CodCliente=v.CUSTNMBR 
JOIN PalmComSync..GP_RUTA R ON c.CodRuta=r.CodRuta
LEFT JOIN PalmComSync..GP_SUBZONA S ON r.CodSubZona=s.CodSubzona
WHERE v.DOCDATE BETWEEN '20110301' AND '20110316' AND v.ITEMNMBR LIKE 'EMB%' 
GROUP BY  
	s.IdZona,
	c.CodCliente,
	v.CUSTNAME,
	c.NombreRuta,
	v.docdate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
