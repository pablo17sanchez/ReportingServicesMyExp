      <Query>
        <DataSourceName>GPDB</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT C.IdZona,c.CodCliente, c.Nombre,R.ITEMNMBR,R.ITEMDESC,SUM(R.QUANTITY)'CANT',SUM(R.XTNDPRCE)'TOTAL' FROM PalmComSync..vw_Clientes_CSR C INNER JOIN GPHN..vw_VentasGP R ON C.CodCliente=R.CUSTNMBR
WHERE C.NombreRuta LIKE '%BA%' AND R.DOCDATE BETWEEN '20110101' AND '20111231'
GROUP BY C.IdZona,c.CodCliente, c.Nombre,R.ITEMNMBR,R.ITEMDESC</CommandText>
        <Timeout>0</Timeout>
      </Query>
