        </Field>
      </Fields>
      <Query>
        <DataSourceName>GPDB</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT c.IdZona,c.ZonaResp,c.NombreSubZona, c.NombreRuta,c.CodCliente,c.Nombre,MONTH(v.DOCDATE)mes,YEAR(v.docdate) ano,SUM(v.QUANTITY)cantidad,SUM(v.XTNDPRCE)Monto FROM vw_VentasGP1 v INNER JOIN  PalmComSync..vw_Clientes_CSR c 
ON v.CUSTNMBR=c.CodCliente
WHERE v.DOCDATE BETWEEN @ifecha AND @afecha AND v.ITEMNMBR LIKE '%EMB%' AND c.IdZona BETWEEN 'GA-01' AND 'GA-09'
GROUP BY c.IdZona,c.ZonaResp,c.NombreSubZona, c.NombreRuta,c.CodCliente,c.Nombre,MONTH(v.DOCDATE),YEAR(v.docdate)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ifecha">
            <Value>=Parameters!ifecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@afecha">
            <Value>=Parameters!afecha.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
