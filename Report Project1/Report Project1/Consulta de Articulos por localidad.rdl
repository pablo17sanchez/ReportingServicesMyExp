    <DataSet Name="cantrecepcion">
      <Query>
        <DataSourceName>Gp</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@articulo">
            <Value>=Parameters!articulo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@localidad">
            <Value>=Parameters!localidad.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     PO1.ITEMNMBR, SUM(PO34.QUANTITY) AS Expr1
FROM         POP10310 AS PO1 INNER JOIN
                      POP10340 AS PO34 ON PO1.POPRCTNM = PO34.POPRCTNM
WHERE     (PO1.ITEMNMBR = @articulo) AND (PO1.LOCNCODE = @localidad)
GROUP BY PO1.ITEMNMBR</CommandText>
      </Query>
      <Fields>
        <Field Name="ITEMNMBR">
          <DataField>ITEMNMBR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Expr1">
