extRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox4</rd:DefaultName><Top>0.33333in</Top><Left>4.10764in</Left><Height>0.25in</Height><Width>1.41667in</Width><ZIndex>4</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Textbox Name="Textbox60"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>=CountRows("cxcbalance")</Value><Style /></TextRun></TextRuns><Style><TextAlign>Left</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox60</rd:DefaultName><Top>0.33333in</Top><Left>5.52431in</Left><Height>0.25in</Height><Width>1in</Width><ZIndex>5</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Textbox Name="Textbox61"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Fecha de Antigüedad:</Value><Style><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox61</rd:DefaultName><Top>0.33333in</Top><Left>6.52431in</Left><Height>0.25in</Height><Width>1.5325in</Width><ZIndex>6</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Textbox Name="fecha"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Parameters!fecha.Value</Value><Style><Format>d</Format></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">fecha</rd:DefaultName><Top>0.33333in</Top><Left>8.05681in</Left><Height>0.25in</Height><Width>1in</Width><ZIndex>7</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><Height>1.34722in</Height><Style /></Body><Width>17.72917in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="cxcbalance"><DataSourceReference>GPDB</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">728f62a3-edc1-4326-81e6-f481465ea721</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="cxcbalance"><Query><DataSourceName>cxcbalance</DataSourceName><QueryParameters><QueryParameter Name="@fecha"><Value>=Parameters!fecha.Value</Value></QueryParameter><QueryParameter Name="@clase"><Value>=Parameters!clase.Value</Value></QueryParameter></QueryParameters><CommandText>select
CM.CUSTNMBR Cod_cliente, 
CM.CUSTNAME Nombre_cliente,

sum(case
when  RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE, @fecha) &lt;31    THEN RM.CURTRXAM
when  RM.RMDTYPAL &gt; 6 AND RM.AGNGBUKT=1 then RM.CURTRXAM *-1
else 0
end) [menor_30],
 
sum(case
when RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE,@fecha) between 31 and 60 then RM.CURTRXAM
 
else 0
end) [31_a_60_Dias],
 
sum(case
when RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE, @fecha) between 61 and 90 then RM.CURTRXAM

else 0
end) [61_a_90_Dias],
 
sum(case
when RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE, @fecha) between 91 and 120 then RM.CURTRXAM

else 0
end) [91_a_120_Dias],
 sum(case
when RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE, @fecha) between 121 and 150 then RM.CURTRXAM
else 0
end) [121_a_150_Dias],

 sum(case
when RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE, @fecha) between 151 and 180 then RM.CURTRXAM
else 0
end) [151_a_180_Dias],

 sum(case
when RM.RMDTYPAL &lt; 7 AND DATEDIFF(d, RM.DOCDATE, @fecha) &gt;180 then RM.CURTRXAM
else 0
end) [181_y_mas],

sum(case
when RM.RMDTYPAL &lt; 7 then RM.CURTRXAM
else RM.CURTRXAM * -1
end) Saldo,
CM.CRLMTAMT Limite_actual,
CM.CUSTCLAS Clases,
ISNULL(CN.CRDTMGR,'') GESTORA,
CASE CM.SALSTERR WHEN 'GA-12' THEN 'BAVARO'
				 WHEN 'GA-13' THEN 'ROMANA'
				 WHEN 'GA-01' THEN 'SAN ISIDRO'
				 WHEN 'GA-02' THEN 'SAN ISIDRO'
				 WHEN 'GA-03' THEN 'SAN ISIDRO'
				 WHEN 'GA-04' THEN 'SAN ISIDRO'
				 WHEN 'GA-05' THEN 'SAN ISIDRO'
				 WHEN 'GA-06' THEN 'SAN ISIDRO'
				 WHEN 'GA-07' THEN 'SAN ISIDRO'
				 WHEN 'GA-08' THEN 'SAN ISIDRO'
				 WHEN 'GA-09' THEN 'SAN ISIDRO'
				 WHEN 'GA-10' THEN 'SAN ISIDRO'
				 WHEN 'GA-11' THEN 'SAN ISIDRO'
				 ELSE ' ' END Localidad,
CS.LASTPYDT Fecha_ultimo_pago,
CS.LPYMTAMT Ultimo_monto_pagado
 
from RM20101 RM
 
inner join RM00101 CM
     on RM.CUSTNMBR = CM.CUSTNMBR
inner join RM00103 CS
     on RM.CUSTNMBR = CS.CUSTNMBR
LEFT JOIN CN00500 CN ON RM.CUSTNMBR=CN.CUSTNMBR
where RM.VOIDSTTS = 0 and RM.CURTRXAM &lt;&gt; 0 
--AND RM.CUSTNMBR NOT LIKE 'C%' 
AND RM.DOCDATE&lt;=@fecha 
AND CM.CUSTCLAS IN (@clase)  
 
group by CM.CUSTNMBR, CM.CUSTNAME, CM.PYMTRMID, CM.CUSTCLAS, 
         CM.CRLMTAMT, CS.LASTPYDT,CS.LPYMTAMT,CN.CRDTMGR,CM.SALSTERR
   HAVING   sum(case
when RM.RMDTYPAL &lt; 7 then RM.CURTRXAM
else RM.CURTRXAM * -1
end) &gt;0       
ORDER BY CM.CUSTNMBR</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Cod_cliente"><DataField>Cod_cliente</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Nombre_cliente"><DataField>Nombre_cliente</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="menor_30"><DataField>menor_30</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ID31_a_60_Dias"><DataField>31_a_60_Dias</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ID61_a_90_Dias"><DataField>61_a_90_Dias</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ID91_a_120_Dias"><DataField>91_a_120_Dias</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ID121_a_150_Dias"><DataField>121_a_150_Dias</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ID151_a_180_Dias"><DataField>151_a_180_Dias</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="ID181_y_mas"><DataField>181_y_mas</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="Saldo"><DataField>Saldo</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="Limite_actual"><DataField>Limite_actual</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="Clases"><DataField>Clases</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="GESTORA"><DataField>GESTORA</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Localidad"><DataField>Localidad</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Fecha_ultimo_pago"><DataField>Fecha_ultimo_pago</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="Ultimo_monto_pagado"><DataField>Ultimo_monto_pagado</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field></Fields></DataSet><DataSet Name="cxclclases"><Query><DataSourceName>cxcbalance</DataSourceName><CommandText>SELECT     RTRIM(CUSTCLAS) AS CUSTCLAS
FROM         RM00101
GROUP BY CUSTCLAS</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="CUSTCLAS"><DataField>CUSTCLAS</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="fecha"><DataType>DateTime</DataType><Prompt>Fecha</Prompt></ReportParameter><ReportParameter Name="clase"><DataType>String</DataType><AllowBlank>true</AllowBlank><Prompt>Clases</Prompt><ValidValues><DataSetReference><DataSetName>cxclclases</DataSetName><ValueField>CUSTCLAS</ValueField><LabelField>CUSTCLAS</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter></ReportParameters><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">49d4c9b7-8067-48e9-a22d-bf76a184654b</rd:ReportID></Report>