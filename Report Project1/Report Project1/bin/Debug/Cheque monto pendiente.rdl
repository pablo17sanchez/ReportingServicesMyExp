quePendiente</DataSourceName><QueryParameters><QueryParameter Name="@idvendor"><Value>=Parameters!idvendor.Value</Value></QueryParameter><QueryParameter Name="@ifecha"><Value>=Parameters!ifecha.Value</Value></QueryParameter><QueryParameter Name="@afecha"><Value>=Parameters!afecha.Value</Value></QueryParameter></QueryParameters><CommandText>  SELECT ISNULL(p32.VENDORID,p33.VENDORID)'IDPROVEDOR',p33.APFRDCNM,ISNULL(p32.DOCAMNT,SUM(p33.ORAPPAMT))'MONTOCHEQUE'
       ,p33.APTODCNM
    ,ISNULL(Apply_To_Document_Amount,p33.APPLDAMT)'FACTURA'
    ,ISNULL((ISNULL(p21.APPLDAMT,p33.APPLDAMT)-(p21.Apply_To_Document_Amount)),0)'MontoPendiente'
,ISNULL(p21.APPLDAMT,p33.APPLDAMT)'MontoAplicadoFactura'


FROM 
PM30300 p33 
LEFT JOIN 
PM30200 p32 ON p33.VCHRNMBR=p32.VCHRNMBR AND p33.VENDORID=p32.VENDORID
LEFT JOIN PM20000 p2 ON p32.VCHRNMBR=p2.VCHRNMBR AND p32.VENDORID=p2.VENDORID AND p32.DOCNUMBR=p2.DOCNUMBR
LEFT JOIN PM20100 p21 ON  p33.APFRDCNM=p21.APFRDCNM  AND p33.VENDORID=p21.VENDORID AND p33.VCHRNMBR=p21.VCHRNMBR
AND p33.APTODCNM=p21.APTODCNM
WHERE     (p33.VENDORID = @idvendor) AND (p33.DOCDATE BETWEEN @ifecha AND @afecha)
GROUP BY p32.VENDORID,
         p33.APFRDCNM,
         p32.DOCAMNT,
         p33.APTODCNM,
         p33.ORAPPAMT ,
         p33.VENDORID,
         p2.CURTRXAM,
         Apply_To_Document_Amount,
         p32.DOCNUMBR
         ,p21.ORAPPAMT
,p33.APPLDAMT
,p21.Apply_From_Document_Amou 
,p21.Apply_To_Document_Amount,p33.ORAPPAMT,
p21.APPLDAMT</CommandText></Query><Fields><Field Name="IDPROVEDOR"><DataField>IDPROVEDOR</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="APFRDCNM"><DataField>APFRDCNM</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="MONTOCHEQUE"><DataField>MONTOCHEQUE</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="APTODCNM"><DataField>APTODCNM</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FACTURA"><DataField>FACTURA</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="MontoPendiente"><DataField>MontoPendiente</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="MontoAplicadoFactura"><DataField>MontoAplicadoFactura</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="ifecha"><DataType>DateTime</DataType><Prompt>Fecha Desde</Prompt></ReportParameter><ReportParameter Name="afecha"><DataType>DateTime</DataType><Prompt>Fecha Hasta</Prompt></ReportParameter><ReportParameter Name="idvendor"><DataType>String</DataType><Prompt>Proveedor</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">eb72dbc9-d530-4020-8f5e-f0001d7cf779</rd:ReportID></Report>