      ,[CodCliente]
      ,[Nombre]
      ,[PERS_CONTACT]
      ,[PHONE1]
      ,[PHONE2]
      ,[CodRuta]
      ,[NombreRuta]
      ,[ZonaResp]
      ,[NOMBREVEND]
      ,[Lunes]
      ,[Martes]
      ,[Miercoles]
      ,[Jueves]
      ,[Viernes]
      ,[Sabado]
      ,[Domingo]
      ,[DATE]
      ,[usuario]
      ,[FECHAMODIF]
      ,[TIPOVENDEDOR]
      ,[ID]
      ,[PALMID]
      ,[IDVENDEDOR]
      ,[FECHA], dbo.fn_devuelveFacturadoPalm(@FECHA, CodCliente, IDVENDEDOR) AS 'FACTURA', dbo.fn_devuelveVisitaPalm(@FECHA, CodCliente, 
                      PALMID) AS 'NO_VISITA'
FROM         DataRS.dbo.vw_ruta_sustitutos_vendedor_h
WHERE     (DATE = @FECHA)  AND (NombreRuta = @nombreRuta)
GROUP BY [Codigo]
      ,[CodCliente]
      ,[Nombre]
      ,[PERS_CONTACT]
      ,[PHONE1]
      ,[PHONE2]
      ,[CodRuta]
      ,[NombreRuta]
      ,[ZonaResp]
      ,[NOMBREVEND]
      ,[Lunes]
      ,[Martes]
      ,[Miercoles]
      ,[Jueves]
      ,[Viernes]
      ,[Sabado]
      ,[Domingo]
      ,[DATE]
      ,[usuario]
      ,[FECHAMODIF]
      ,[TIPOVENDEDOR]
      ,[ID]
      ,[PALMID]
      ,[IDVENDEDOR]
      ,[FECHA]
ORDER BY CodRuta</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Codigo"><DataField>Codigo</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="CodCliente"><DataField>CodCliente</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Nombre"><DataField>Nombre</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CodRuta"><DataField>CodRuta</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="PERS_CONTACT"><DataField>PERS_CONTACT</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="NombreRuta"><DataField>NombreRuta</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="PHONE1"><DataField>PHONE1</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Lunes"><DataField>Lunes</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Boolean</rd:TypeName></Field><Field Name="PHONE2"><DataField>PHONE2</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Martes"><DataField>Martes</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Boolean</rd:TypeName></Field><Field Name="Miercoles"><DataField>Miercoles</DataField><rd:TypeName x