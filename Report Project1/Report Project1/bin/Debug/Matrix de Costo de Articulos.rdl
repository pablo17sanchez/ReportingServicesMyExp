ems><Height>2in</Height><Style /></Body><Width>6.5in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="Mcosto"><DataSourceReference>GPDB</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">cee666a5-5225-4cc4-b64d-ee9433a92077</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="McostoArt"><Query><DataSourceName>Mcosto</DataSourceName><QueryParameters><QueryParameter Name="@IFECHA"><Value>=Parameters!IFECHA.Value</Value></QueryParameter><QueryParameter Name="@AFECHA"><Value>=Parameters!AFECHA.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT  DISTINCT   ITEMNMBR, UNITCOST, MONTH(DATERECD) AS 'MES',DAY(DATERECD) AS 'DAY',CASE PCHSRCTY WHEN 1 THEN 'Ajuste '
                                                                                                     WHEN 2 THEN 'Varianza '
                                                                                                     WHEN 3 THEN 'Transferencia '
                                                                                                     WHEN 4 THEN 'Anulación'
                                                                                                     WHEN 5 THEN 'Recepción '
                                                                                                     WHEN 6 THEN 'Volver'
                                                                                                     WHEN 7 THEN 'Asamblea'
                                                                                                     WHEN 8 THEN 'en tránsito'
                                                                                                     
                                                                                                     END'TIPO_TRANSF'
