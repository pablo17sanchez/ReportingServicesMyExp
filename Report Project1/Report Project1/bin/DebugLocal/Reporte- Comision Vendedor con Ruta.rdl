  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ComisionVentaRuta</DataSourceName>
        <CommandText>--SCRIPT VENDEDORES EN RUTA
SELECT 
	e.SubGrupo_Trabajo,
	e.Desc_SubGrupo,
	e.Numero,
	e.Nom_Emp,
	v.ITEMNMBR,
	v.UNITPRCE,
	SUM(v.QUANTITY)'QUANTITY',
	SUM(V.XTNDPRCE)'TOTAL',
	 CASE 
			 --PT100
			 WHEN V.ITEMNMBR = 'PT100' AND V.UNITPRCE &lt;= 30 THEN SUM(V.XTNDPRCE) * 0.025 
			 WHEN V.ITEMNMBR = 'PT100' AND V.UNITPRCE &gt;= 31 THEN SUM(V.QUANTITY) * 0.75 
			 --PT200
			 WHEN V.ITEMNMBR = 'PT200' OR  V.ITEMNMBR = 'PT204' THEN SUM(V.XTNDPRCE) * 0.025 
			 --PT300
			 WHEN V.ITEMNMBR = 'PT300' AND V.UNITPRCE &lt;= 27 THEN SUM(V.XTNDPRCE) * 0.10 
			 WHEN V.ITEMNMBR = 'PT300' AND V.UNITPRCE &gt;= 28 THEN SUM(V.QUANTITY) * 2.7 
			 --PT400
			 WHEN V.ITEMNMBR = 'PT400'  THEN SUM(V.XTNDPRCE) * 0.04 
			 WHEN V.ITEMNMBR = 'PT400' AND V.UNITPRCE &lt;=87 THEN SUM(V.XTNDPRCE) * 0.02 
			 WHEN V.ITEMNMBR = 'PT400' AND V.UNITPRCE &gt;=88 THEN SUM(V.XTNDPRCE) * 0.04
			 
			 --PT500
			 WHEN V.ITEMNMBR = 'PT500'  THEN SUM(V.XTNDPRCE) * 0.04 
            
			 --PT600
			 WHEN V.ITEMNMBR = 'PT600' THEN SUM(V.XTNDPRCE) * 0.08 
			 
			 --EMBUTIDO
			 WHEN V.ITEMNMBR = 'EMB100' OR V.ITEMNMBR = 'EMB200' OR V.ITEMNMBR = 'EMB201' THEN SUM(V.XTNDPRCE) * 0.03 
             --COMBO
			 WHEN V.ITEMNMBR = 'COMBO-100' OR V.ITEMNMBR = 'COMBO-101' OR V.ITEMNMBR = 'COMBO-102' THEN SUM(V.XTNDPRCE) * 0.0225 
			 ELSE 0 END AS 'Comision' 
			 
			 
FROM dbo.View_VentasGP2 as v INNER JOIN Personal_Nomina..vw_Empleados as e ON v.IdVendedor=e.Numero
WHERE v.DOCDATE BETWEEN '20100816'AND '20100831' 
	AND e.SubGrupo_Trabajo IN ('121','162','220')
	AND v.IdVendedor&lt;&gt;'D001'
	AND (V.ITEMNMBR &lt;&gt; 'OT006') 
	AND (V.ITEMNMBR &lt;&gt; 'KIT300')
GROUP BY 
	e.SubGrupo_Trabajo,
	e.Desc_SubGrupo,
	e.Numero,
	e.Nom_Emp,
	v.ITEMNMBR,
	v.UNITPRCE</CommandText>
      </Query>
