    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>NM</DataSourceName>
        <CommandText>SELECT     RTRIM(RTRIM(Item) + '\' + RTRIM(Descripcion)) AS Productos, RTRIM(Item) AS ITEM
FROM         PalmComSync.dbo.vw_Productos
GROUP BY Item, Descripcion
ORDER BY Item</CommandText>
      </Query>
      <Fields>
        <Field Name="Productos">
