  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Des">
            <Value>=Parameters!Des.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ifecha">
            <Value>=Parameters!ifecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@afecha">
            <Value>=Parameters!afecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@emp">
            <Value>=Parameters!emp.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF (@emp IS NULL)
BEGIN
SELECT     E.Numero, 
			E.Nom_Emp, 
			de.Tipo_Descuento, 
			d.Descripcion, 
			de.Valor, 
			cd.Valor_Cuota, 
			cd.Numero_Cuotas, 
			cd.Cuotas_Pagadas, 
			CASE ISNULL(cd.Monto_Adeudado,0) WHEN 0 THEN de.Valor ELSE cd.Monto_Adeudado END 'Monto_Adeudado', 
            cd.Valor AS 'Valor_prestamos', 
            cd.Fecha AS 'Fecha_trasn', 
            N.Fecha_Desde, 
            N.Fecha_Hasta, 
            cd.Deduccion
FROM         Descuentos_Nomina_Empleados AS de INNER JOIN
                      Nomina_General AS N ON de.Codigo_nomina = N.Codigo_Nomina INNER JOIN
                      vw_Empleados AS E ON de.Codigo_empleado = E.Numero INNER JOIN
                      Tipo_Descuentos AS d ON de.Tipo_Descuento = d.Codigo LEFT OUTER JOIN
                      vw_Descuentos AS cd ON de.Codigo_Descuento = cd.Codigo_Descuento
WHERE     (de.Tipo_Descuento IN (@Des)) AND (N.Fecha_Desde = @ifecha)  OR
                      (N.Fecha_Hasta = @afecha)
GROUP BY E.Numero, E.Nom_Emp, de.Tipo_Descuento, d.Descripcion, de.Valor, cd.Valor_Cuota, cd.Numero_Cuotas, cd.Cuotas_Pagadas, cd.Monto_Adeudado, cd.Valor, 
                      cd.Fecha, N.Fecha_Desde, N.Fecha_Hasta, cd.Deduccion
ORDER BY E.Numero
END

ELSE IF (@emp&lt;&gt;0)
BEGIN
SELECT     E.Numero, 
			E.Nom_Emp, 
			de.Tipo_Descuento, 
			d.Descripcion, 
			de.Valor, 
			cd.Valor_Cuota, 
			cd.Numero_Cuotas, 
			cd.Cuotas_Pagadas, 
			CASE ISNULL(cd.Monto_Adeudado,0) WHEN 0 THEN de.Valor ELSE cd.Monto_Adeudado END 'Monto_Adeudado', 
            cd.Valor AS 'Valor_prestamos', 
            cd.Fecha AS 'Fecha_trasn', 
            N.Fecha_Desde, 
            N.Fecha_Hasta, 
            cd.Deduccion
FROM         Descuentos_Nomina_Empleados AS de INNER JOIN
                      Nomina_General AS N ON de.Codigo_nomina = N.Codigo_Nomina INNER JOIN
                      vw_Empleados AS E ON de.Codigo_empleado = E.Numero INNER JOIN
                      Tipo_Descuentos AS d ON de.Tipo_Descuento = d.Codigo LEFT OUTER JOIN
                      vw_Descuentos AS cd ON de.Codigo_Descuento = cd.Codigo_Descuento
WHERE     (de.Tipo_Descuento IN (@Des)) AND (N.Fecha_Desde = @ifecha) AND (E.Numero = @emp) OR
                      (N.Fecha_Hasta = @afecha)
GROUP BY E.Numero, E.Nom_Emp, de.Tipo_Descuento, d.Descripcion, de.Valor, cd.Valor_Cuota, cd.Numero_Cuotas, cd.Cuotas_Pagadas, cd.Monto_Adeudado, cd.Valor, 
                      cd.Fecha, N.Fecha_Desde, N.Fecha_Hasta, cd.Deduccion
ORDER BY E.Numero
END</CommandText>
