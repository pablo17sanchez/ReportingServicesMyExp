  <DataSets>
    <DataSet Name="dsDynamicsGP">
      <Fields>
        <Field Name="Cliente">
          <DataField>Cliente</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nombre">
          <DataField>Nombre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UltimaVenta">
          <DataField>UltimaVenta</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DeployDS</DataSourceName>
        <CommandText>select custnmbr Cliente, custname Nombre, max(docdate) UltimaVenta
from
(select  r.custnmbr, r1.custname, max(docdate) docdate
from sop30200 r with (nolock) join rm00101 r1 with (nolock) on (r.custnmbr=r1.custnmbr)
where soptype = 3 and docamnt &gt; 0 and voidstts = 0 and r1.inactive = 0 and r1.hold=0
--and r.docdate &lt; (select max(docdate) from sop10100 where docdate &lt; getdate())-8
group by r.custnmbr, r1.custname
union all
select s.custnmbr, r.custname, max(docdate) docdate
from sop10100 s with (nolock) join rm00101 r with (nolock) on (s.custnmbr=r.custnmbr)
where voidstts=0 and soptype=3 and docamnt&gt;0 and r.inactive=0 and r.hold=0
--and s.docdate &lt; (select max(docdate) from sop10100 where docdate &lt; getdate())-8
group by s.custnmbr, r.custname
) as P
group by custnmbr, custname
having max(docdate) &lt; getdate() - 8</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
