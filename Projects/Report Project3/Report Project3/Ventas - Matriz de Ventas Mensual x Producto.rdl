      ,[ZonaResp]
      ,[NombreSupervisor]
      ,[CodSubZona]
  FROM [TWO].[dbo].[vw_cte_vta_CSR]
WHERE DOCDATE between @iFecha and @aFecha and ITEMNMBR = @Articulo</CommandText>
