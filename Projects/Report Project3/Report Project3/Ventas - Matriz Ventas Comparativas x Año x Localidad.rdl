        <Field Name="Monto">
          <DataField>Monto</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DeployDS</DataSourceName>
        <CommandText>select LocnCode Localidad,year(docdate)Year,Month(docdate)Month,ITEMNMBR +' '+ITEMDESC Articulo ,sum(quantity)Cantidad,sum(xtndprce)Monto from vw_ventasgp
where year(docdate) between @iYear and @aYear and itemnmbr = @Articulo
group by LocnCode, year(docdate),Month(docdate),ITEMNMBR +' '+ITEMDESC
order by year(docdate),Month(docdate)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@iYear">
            <Value>=Parameters!iYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@aYear">
            <Value>=Parameters!aYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Articulo">
            <Value>=Parameters!Articulo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
