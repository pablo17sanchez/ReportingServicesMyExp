  <DataSets>
    <DataSet Name="VentasGEASA">
      <Query>
        <DataSourceName>GPIND</DataSourceName>
        <CommandText>select rm.custnmbr[NumeroCliente],rm.Custname[NombreCliente],rtrim(sop.itemnmbr)[NumeroArticulo],rtrim(sop.itemdesc)[DescArticulo],sum(sop.quantity)[Cantidad],sop.unitprce[PrecioUnitario],sum(sop.xtndprce)[Monto],docdate[FechaDoc],case rm.custclas when 'Distribuidor' Then 'Venta de Distriuidor' else case sop.docid When 'PALMFACTURA' Then 'Venta Ranchera' When 'MANUAL' Then 'Venta Ranchera'  else 'Venta en Planta' end end [CanaldeServicio] ,sop.locncode[Localidad] from gphn..rm00101 rm join (select a.voidstts,a.custnmbr,b.soptype,b.sopnumbe,b.itemnmbr,b.itemdesc,b.quantity,b.unitprce,b.xtndprce,a.docdate,a.docid,a.locncode from gphn..sop10100 a join gphn..sop10200 b on 
a.sopnumbe = b.sopnumbe
where a.soptype = 3 and voidstts = 0
union 
select c.voidstts,c.custnmbr,d.soptype,D.sopnumbe,d.itemnmbr,d.itemdesc,d.quantity,d.unitprce,d.xtndprce,c.docdate,c.docid,c.locncode from gphn..sop30200 c join gphn..sop30300 d on 
c.sopnumbe = d.sopnumbe
where c.soptype = 3 and voidstts = 0 ) sop on rm.custnmbr = sop.custnmbr
where docdate between @aFecha  and @iFecha and sop.itemnmbr = @iArticulo and sop.locncode = @Localidad
group by</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
